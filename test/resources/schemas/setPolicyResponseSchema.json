{
    "title": "set-policy response schema",
    "type": "object",
    "required": ["success", "message", "data"],
    "properties": {
        "success": {
            "type": "boolean"
        },
        "message": {
            "type": "string"
        },
        "data": {
            "type": "object",
            "required": ["id", "esbd_id", "request_number", "policy_number", "global_id", "status", "holders", "code", "payment_system", "payment_link", "contract_file"],
            "properties": {
                "id": {
                    "type": "number"
                },
                "esbd_id": {
                    "type": "number"
                },
                "request_number": {
                    "type": "string"
                },
                "policy_number": {
                    "type": "string"
                },
                "global_id": {
                    "type": "string"
                },
                "status": {
                    "type": "number"
                },
                "holders": {
                    "type": "array",
                    "minItems": 1,
                    "uniqueItems": true,
                    "items": {
                        "type": "object",
                        "required": ["external_id", "premium", "premium_rate", "amount_sum", "amount_cur"],
                        "properties": {
                            "external_id": {
                                "type": "number"
                            },
                            "premium": {
                                "type": "string"
                            },
                            "premium_rate": {
                                "type": "number"
                            },
                            "amount_sum": {
                                "type": "number"
                            },
                            "amount_cur": {
                                "type": "string"
                            }
                        }
                    }
                },
                "code": {
                    "type": "string"
                },
                "payment_system": {
                    "type": "string"
                },
                "payment_link": {
                    "type": "string"
                },
                "contract_file": {
                    "type": "string"
                }
            }
        }
    }
}